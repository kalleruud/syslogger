# Syslogger Backend Configuration

# HTTP server port for web UI and API
PORT=3000

# UDP port for receiving syslog messages
SYSLOG_PORT=5140

# Node environment (development, production)
NODE_ENV=development

# Database connection URL (using SQLite for simplicity)
DATABASE_URL = "./data/syslogger.db"

# CORS origin for API access (e.g., "*" for all origins or "http://localhost:3000" for specific origin)
CORS_ORIGIN = "*"
